{
  "name": "svelte-hugo-app",
  "version": "1.0.1",
  "devDependencies": {
    "rollup": "^1.27.13",
    "rollup-plugin-commonjs": "^10.1.0",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "rollup-plugin-svelte": "^5.1.1",
    "rollup-plugin-terser": "^5.1.3",
    "svelte": "^3.16.5"
  },
  "scripts": {
    "build": "rollup -c ; hugo",
    "dev": "rollup -c -w",
    "start": "hugo server --bind=0.0.0.0",
    "start:dev": "hugo server --disableFastRender --bind=0.0.0.0 -D"
  }
}
